#include <stdio.h>

int main(){


int a,b,n;

printf("  Enter the value :  ");

scanf(" %d",&n);

printf("  Enter the value   a     :  ");
scanf(" %d",&a,&b);

printf("  Enter the value    b :  ");
scanf(" %d",&b);

if (n%a==0 && n%b==0)

{



    printf("%d is disibile by both  %d  and %d ",n ,a,b);

}




else if(n%a==0){

      printf("%d is disibile by a  %d   ",n ,a);




}



else if(n%b==0){

      printf("%d is disibile by b %d  ",n ,b);




}

   else {



        printf("%d is not disibile by both  %d  and %d ",n ,a,b);



   }


}
