#include <stdio.h>
 int main (){



 //double num;
 int num;

 scanf(" %d",&num);

 /*
 0-32 → F
33-39 → D
40-49 → C
50-59 →B
60-69 →A
70-79 → A
80-100 →A+

 */

 if ( num<=32)

 {
     printf("number %d and  your Grade is F",num);
 }


else if ( num>=33 &&num<=39)

 {
     printf("number %d and your Grade is D",num);
 }



else if ( num>=50 &&num<=59)

 {
     printf(" number %d and your Grade is C",num);
 }





 else if ( num>=60 &&num<=69)

 {
     printf("number %d  and your Grade is B",num);
 }


 else if ( num>=70 &&num<=79)

 {
     printf("number %d and your Grade is A",num);
 }





 else if ( num>=80 &&num<=100)

 {
     printf("number %d and  your Grade is A+",num);
 }



 else
    printf("invalide number");

 return 0;

 }
