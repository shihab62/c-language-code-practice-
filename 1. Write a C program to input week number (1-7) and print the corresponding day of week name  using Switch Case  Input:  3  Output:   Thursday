#include <stdio.h>

int main (){

int num;


scanf("%d",&num);

if (num==1){


  printf("sunday");




}
else if  (num==2){


  printf("monday");




}else if  (num==3){


  printf("tuesday");




}
else if  (num==4){


  printf("wednessday");




}

else if  (num==5){


  printf("Tursday");




}

else if  (num==6){


  printf("friday");




}
else if  (num==7){


  printf("saturday");




}
else
  printf("plase input 1 to 7 range number");
  getch();

return 0;


}
